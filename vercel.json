{
  "version": 2,
  "name": "经典游戏盒",
  "builds": [
    {
      "src": "build/web-mobile/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "X-Content-Type-Options": "nosniff"
      }
    }
  ],
  "buildCommand": "echo '跳过构建，直接部署静态文件'",
  "outputDirectory": "build/web-mobile",
  "framework": null
}