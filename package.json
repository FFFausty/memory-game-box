{
  "name": "memory-game-box",
  "version": "1.0.0",
  "description": "经典游戏盒 - 包含神经衰弱记忆游戏等多款经典游戏",
  "main": "main.js",
  "scripts": {
    "build": "echo '静态文件已构建完成，无需远程构建'",
    "build:debug": "cocos build --platform web-mobile --config build-config.json --debug",
    "build:wechat": "cocos build --platform wechatgame --config build-config-wechat.json",
    "serve": "npx serve build/web-mobile -p 8080",
    "clean": "rm -rf build/",
    "preview": "npm run build && npm run serve",
    "vercel-build": "echo '本地构建完成，直接部署build/web-mobile目录即可'"
  },
  "keywords": [
    "memory-game",
    "cocos-creator",
    "webgl",
    "game-box",
    "classic-games"
  ],
